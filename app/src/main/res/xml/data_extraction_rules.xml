<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Keep device identifiers and session logs local-only. -->
        <exclude domain="sharedpref" path="ergometer_app_settings.xml" />
        <exclude domain="file" path="." />
        <exclude domain="database" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Apply the same policy during cable/wireless device transfer. -->
        <exclude domain="sharedpref" path="ergometer_app_settings.xml" />
        <exclude domain="file" path="." />
        <exclude domain="database" path="." />
    </device-transfer>
</data-extraction-rules>
